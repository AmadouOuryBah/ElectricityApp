@model Electricity.BusinessLogic.Requests.ElectricalEquipementRequest

<h2 class="mt-3">Create new Electrical Equipement</h2>

<form class="row g-3 my-4" method="post" asp-controller="ElectricalEquipement">
    <div class="mb-3">
        <label asp-for="Name" class="form-label">Name</label>
        <input asp-for="Name" type="text" class="form-control">
    </div>

    <div class="mb-3">
        <label asp-for="Power" class="form-label">Power</label>
        <input asp-for="Power" type="text" class="form-control">
    </div>

    <div class="mb-3">
        <label asp-for="Coefficient" class="form-label">Coefficient</label>
        <input asp-for="Coefficient" type="text" class="form-control">
    </div>

    <div>
        <button type="submit">Add</button>
    </div>
</form>
