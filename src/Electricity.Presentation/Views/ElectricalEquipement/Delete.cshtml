@model Electricity.BusinessLogic.DTO_s.ElectricalEquipementDto

<h2 class="mt-3">Are You Sure you want to Delete <br /> an Electrical Equipement ?</h2>



<form class="row g-3 my-4" method="post" asp-controller="ElectricalEquipement">
    <div class="row">
        <div class="mb-3 col-6">
            <label asp-for="Name" class="form-label">Name</label>
            <input asp-for="Name" type="text" value=@Model.Name disabled class="form-control">
        </div>
    </div>
   
    <div class="row">
        <div class="mb-3 col-6">
            <label asp-for="Power" class="form-label">Power</label>
            <input disabled asp-for="Power" value=@Model.Power type="text" class="form-control">
        </div>
    </div>
   
    <div class="row">
        <div class="mb-3 col-6">
            <label asp-for="Coefficient" class="form-label">Coefficient</label>
            <input asp-for="Coefficient" disabled type="text" value=@Model.Coefficient class="form-control">
        </div>
    </div>
  
    <div class="row">
        <div>
            <button type="submit" class="btn btn-primary"> Delete </button>
            <a  asp-controller="ElectricalEquipement" asp-action="Index"  class="btn btn-secondary mx-2">Back</a>
        </div>
    </div>
    

</form>
