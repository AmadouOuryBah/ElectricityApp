@model IEnumerable<Electricity.BusinessLogic.DTO_s.ElectricityMeterDto>
<div class="row">
    <div class="col-lg-6  mt-3">
        <h2>Electricity Meter</h2>
    </div>
   

</div>

<div>
    <a class="btn btn-primary" asp-action="Create" asp-controller="ElectricityMeter">create</a>
</div>
<table class="table my-4">
   
    <thead class="table-dark">

        <tr>
            <th scope="col">Name</th>
            <th scope="col">Factory Number</th>
            <th scope="col">Action</th>
          
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.FactoryNumber</td>
                <td>
                    <a class="btn btn-secondary" href="ElectricityMeter/Edit/@item.Id">edit</a>
                    <a class="btn btn-danger mx-2" href=""><i class="bi bi-trash3-fill"></i>delete</a>
                </td>
            </tr>
           
        }
    </tbody>
   
</table>