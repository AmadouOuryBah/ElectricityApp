@model IEnumerable<Electricity.BusinessLogic.DTO_s.ElectricityMeterDto>


<h2>List of electrical meters by renters </h2>
<table class="table my-4">
    
    
    <thead class="table-dark">
       
        <tr>
          <th scope="col">name</th>
          <th scope="col">power</th>
          <th scope="col">quantity</th>
          <th scope="col">coeffcient</th>
          <th scope="col">room nber</th>
          <th scope="col">address</th>
          <th scope="col">city</th>
           <th scope="col">renter</th>
           <th scope="col">actions</th>
       </tr>

    </thead>
    <tbody>
        @foreach (var electricalMeter in Model)
        {
            <tr>
                <td>@electricalMeter.Name</td>
                <td>@electricalMeter.Power</td>
                <td>@electricalMeter.NumberOfDevice</td>
                <td>@electricalMeter.Coefficient</td>
                <td>@electricalMeter.Room.NumberOfRoom</td>
                <td>@electricalMeter.Room.Building.Address</td>
                <td>@electricalMeter.Room.Building.City</td>
                <td>@electricalMeter.Room.Renter.Name</td>
                <td ><a class="btn btn-secondary mx-2" href="ElectricalMeter/Edit/@electricalMeter.Id">edit</a>
                    <a class="btn btn-danger" href="ElectricalMeter/Delete/@electricalMeter.Id"><i class="bi bi-trash3-fill"></i>delete</a></td>
            </tr>
        }
    </tbody>

    <div>
        <a class="btn btn-primary-lg mx-2" asp-action="Create" asp-controller="ElectricalMeter">create</a>
         
    </div>
</table>

