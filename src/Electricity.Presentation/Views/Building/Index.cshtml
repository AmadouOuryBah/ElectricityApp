@model IEnumerable<Electricity.BusinessLogic.DTO_s.BuildingDto>

<table class="table my-4">
    <caption>list of buildings </caption>
    <thead class="table-dark">
        
        <tr>
            <th scope="col">Name</th>
            <th scope="col">Address</th>
            <th scope="col">Floor</th>
            <th scope="col">City</th>
            <th scope="col">Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var building in Model)
        {
           
            <tr>
                <td>@building.Name</td>
                <td>@building.Address</td>
                <td>@building.Floor</td>
                <td>@building.City</td>
                <td><a class="btn btn-secondary mx-2" href="Building/Edit/@building.Id">edit</a> <a class="btn btn-danger" href=""><i class="bi bi-trash3-fill"></i>delete</a></td>
            </tr>
           
        }
    </tbody>
    <a btn btn-primary-lg asp-action="Create" asp-controller="Building">create</a>
</table>